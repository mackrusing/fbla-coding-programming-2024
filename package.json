{
  "name": "2024-coding-programming-bun",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "bunx prettier -w ./",
    "db-generate": "bunx drizzle-kit generate:sqlite --out db/migrations --schema src/db/schema.ts",
    "db-migrate": "bunx drizzle-kit push:sqlite --schema src/db/schema.ts --driver better-sqlite --url db/sqlite.db",
    "db-reset": "curl -X PUT localhost:3000/utils/reset-db",
    "db-studio": "bunx drizzle-kit studio"
  },
  "trustedDependencies": [
    "better-sqlite3"
  ],
  "dependencies": {
    "better-sqlite3": "^9.4.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.7",
    "lucide-react": "^0.364.0",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
